[core]
  whitespace = space-before-tab,-indent-with-non-tab,trailing-space
[color "branch"]
  current = yellow
  local = green
  remote = magenta
  upstream = bold cyan
[color "diff"]
  meta = cyan bold
  frag = normal bold
  old = red
  new = green
  whitespace = magenta reverse
[color "status"]
  added = green
  changed = yellow
  untracked = red
[commit]
  gpgsign = true
[diff]
  renames = copies
[help]
  autocorrect = 10
