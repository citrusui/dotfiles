[core]
  whitespace = space-before-tab,-indent-with-non-tab,trailing-space
[color "branch"]
  current = yellow
  local = green
  remote = magenta
  upstream = bold cyan
[color "diff"]
  meta = yellow bold
  frag = cyan bold
  old = magenta
  new = green
  whitespace = red reverse
[color "status"]
  added = yellow
  changed = green
  untracked = cyan
[commit]
  gpgsign = true
[diff]
  renames = copies
[help]
  autocorrect = 10