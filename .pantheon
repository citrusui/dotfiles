gsettings set org.pantheon.terminal.settings allow-bold "true"
gsettings set org.pantheon.terminal.settings alt-changes-tab "true"
gsettings set org.pantheon.terminal.settings audible-bell "true"
gsettings set org.pantheon.terminal.settings background "#40484f"
gsettings set org.pantheon.terminal.settings cursor-color "#6b7a86"
gsettings set org.pantheon.terminal.settings cursor-shape "Block"
gsettings set org.pantheon.terminal.settings encoding "UTF-8"
gsettings set org.pantheon.terminal.settings follow-last-tab "true"
gsettings set org.pantheon.terminal.settings font "Ubuntu Mono 13"
gsettings set org.pantheon.terminal.settings foreground "#f9f9f9"
gsettings set org.pantheon.terminal.settings natural-copy-paste "true"
gsettings set org.pantheon.terminal.settings palette "#000:#e03431:#79ce80:#e8e366:#4ea5d9:#e031a8:#4ed4d8:#f9f9f9:#000:#e03431:#79ce80:#e8e366:#4ea5d9:#e031a8:#4ed4d8:#f9f9f9"
gsettings set org.pantheon.terminal.settings remember-tabs "true"
gsettings set org.pantheon.terminal.settings save-exited-tabs "true"
gsettings set org.pantheon.terminal.settings shell "bash"
gsettings set org.pantheon.terminal.settings scrollback-lines "-1"
gsettings set org.pantheon.terminal.settings tab-bar-behavior "Always Show Tabs"
gsettings set org.pantheon.terminal.settings unsafe-paste-alert "true"
echo "Terminal will now quit for changes to take effect."; sleep 10;
killall pantheon-terminal
